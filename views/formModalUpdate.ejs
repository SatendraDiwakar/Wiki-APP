<form id="<%= id %>" class="insert-article-form">
  <button type="button" id="fClose<%= id %>" class="btn fClose-btn">X</button>
  <input
    type="text"
    name="titleU"
    id="title<%= id %>"
    class="input-article"
    placeholder="Title"
    autocomplete="off"
  />
  <textarea
    type="text"
    name="contentU"
    id="content<%= id %>"
    class="input-article content"
    placeholder="Content"
    autocomplete="off"
  ></textarea>
  <button type="submit" class="insert-article-btn">Update</button>
</form>
